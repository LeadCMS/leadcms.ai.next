{
  "name": "leadcms.ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fetch:leadcms": "node ./scripts/fetch-leadcms-content.mjs",
    "generate:env": "node ./scripts/generate-env-js.mjs",
    "prebuild": "npm run fetch:leadcms && npm run generate:env",
    "predev": "npm run prebuild",
    "prestart": "npm run prebuild",
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "serve": "npx serve out",
    "docker:build": "npm run build && docker build -t leadcms_site .",
    "docker:run": "docker container rm leadcms_site 2>/dev/null || true && docker run -p 3000:8080 --env-file .env --name leadcms_site leadcms_site",
    "docker:preview:build": "docker build -f preview/Dockerfile -t leadcms_site_preview .",
    "docker:preview:run": "docker container rm leadcms_site_preview 2>/dev/null || true && docker run -p 3000:80 --name leadcms_site_preview --env-file .env leadcms_site_preview",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,mjs}\" --ignore-path .prettierignore",
    "lint": "next lint"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
