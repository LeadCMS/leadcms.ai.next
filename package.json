{
  "name": "leadcms.ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "pull": "leadcms pull",
    "watch": "leadcms watch",
    "livepreview": "pnpm run dev",
    "generate:env": "node ./scripts/generate-env-js.mjs",
    "scan-icons": "node ./scripts/scan-icons.mjs",
    "prebuild": "leadcms fetch && leadcms generate-env && pnpm run scan-icons",
    "predev": "pnpm run prebuild",
    "prestart": "pnpm run prebuild",
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "serve": "pnpm dlx serve out",
    "docker:build": "pnpm run build && docker build -t leadcms_site .",
    "docker:run": "docker container rm leadcms_site --force || true && docker run -p 3000:8080 --env-file .env --name leadcms_site leadcms_site",
    "docker:preview:build": "docker build -f preview/Dockerfile -t leadcms_site_preview .",
    "docker:preview:run": "docker container rm leadcms_site_preview --force || true && docker run -p 3000:80 --name leadcms_site_preview --env-file .env leadcms_site_preview",
    "format": "prettier --write \"./**/*.{ts,tsx,js,jsx,mjs}\" --ignore-path .prettierignore",
    "lint": "next lint"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
